-# FIND SERVICES DATE RANGE
=f.input :created_at_from_date, required: false,
  input_html: {class: 'datepicker'}, 
  as: :string, label: "From Date"
=f.input :created_at_to_date, required: false,
  input_html: {class: 'datepicker'}, 
  as: :string, label: "To Date"

-# FIND SERVICES REGION BUILDER
=render partial: 'admin/services/region_builder',
  locals: { wrapper_id: 'find_services_origin_recipe', label_text: "Origin"}
=f.input :find_services_origin_recipe, as: :hidden,
  input_html: { value: '[]'}

:javascript
  $(document).ready(function() {
    // Set up serializer and autocomplete for region builders

    var tripOriginRecipe = new M.Recipe({
      container: $('#find_services_origin_recipe'),
      recipeInput: $('#download_table_find_services_origin_recipe'),
      searchPath: '#{admin_autocomplete_path}',
      ingredientLabelTag: '#{escape_javascript render partial: "admin/geographies/ingredient_label"}'
    });

  });
