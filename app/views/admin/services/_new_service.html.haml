=content_for :header do
  = "Create a new Service"
=content_for :body do
  = f.input :name, required: true, :as => :string
  = f.input :type, required: true, collection: ['Transit', 'Paratransit'], include_blank: "Select a Service Type"
=content_for :footer do
  = f.submit "OK", class: 'btn btn-primary' #, role: 'button'
