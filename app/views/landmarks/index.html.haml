%h1.text-center
  Landmarks


%div.col-md-1
%div.col-md-10
  %table.table
    %tr
      %th
        Name
      %th
        Street Number
      %th
        Route
      %th
        Address
      %th 
        City
      %th
        State
      %th
        Zip
      %th 
        Lat
      %th
        Lng
      %th
        Types
    -@landmarks.each do |landmark|
      %tr
        %td
          =landmark.name 
        %td
          =landmark.street_number
        %td
          =landmark.route
        %td
          =landmark.address
        %td
          =landmark.city
        %td
          =landmark.state
        %td
          =landmark.zip
        %td
          =landmark.lat
        %td
          =landmark.lng
        %td
          =landmark.types
%div.col-md-1


